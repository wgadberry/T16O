                     "RpcExchange":  "rpc.topic",
                     "TaskExchange":  "tasks.topic"
                 },
  "Database": {
              "ConnectionString": "Server=localhost;Port=3306;Database=t16o_db;User=root;Password=rootpassword;Allow User Variables=True;Min Pool Size=10;Max Pool Size=50;Default Command Timeout=120;"
  },
    "Solana":  {
                   "TransactionRpcUrls":  [
                                   "https://solana-mainnet.core.chainstack.com/d0eda0bf942f17f68a75b67030395ceb",
                                   "https://mainnet.helius-rpc.com/?api-key=684225cd-056a-44b5-b45d-8690115ae8ae"
                               ],
                   "AssetRpcUrls":  [
                                   "https://mainnet.helius-rpc.com/?api-key=684225cd-056a-44b5-b45d-8690115ae8ae"
                               ],
                   "RpcEndpoints": [
                       {
                           "Name": "Chainstack",
                           "Url": "https://solana-mainnet.core.chainstack.com/d0eda0bf942f17f68a75b67030395ceb",
                           "MaxRps": 25,
                           "MaxConcurrent": 8,
                           "Priority": 1
                       },
                       {
                           "Name": "Helius",
                           "Url": "https://mainnet.helius-rpc.com/?api-key=684225cd-056a-44b5-b45d-8690115ae8ae",
                           "MaxRps": 50,
                           "MaxConcurrent": 15,
                           "Priority": 2
                       }
                   ]
               },
    "Fetcher": {
        "Transaction": {
            "MaxConcurrentRequests": 23,
            "RateLimitMs": 0,
            "MaxRetryAttempts": 3,
            "InitialRetryDelayMs": 1000
        },
        "Asset": {
            "MaxConcurrentRequests": 1,
            "RateLimitMs": 100,
            "EnableFallbackChain": true
        }
    },
    "InfluxDB": {
        "Enabled": true,
        "Url": "http://localhost:8086",
        "Token": "t16o-metrics-token",
        "Org": "t16o",
        "Bucket": "metrics",
        "FlushIntervalMs": 5000,
        "BatchSize": 100
    },
    "Workers":  {
                    "Defaults": {
                        "Prefetch": 1,
                        "Concurrency": 1,
                        "RateLimitMs": 0
                    },
                    "TransactionFetch":  {
                                             "Enabled":  true,
                                             "QueueName":  "tx.fetch",
                                             "Prefetch": 10
                                         },
                    "TransactionFetchSite":  {
                                                 "Enabled":  true,
                                                 "QueueName":  "tx.fetch.site",
                                                 "Prefetch": 1
                                             },
                    "TransactionFetchDb":  {
                                               "Enabled":  true,
                                               "QueueName":  "tx.fetch.db",
                                               "Prefetch": 10,
                                               "AssessMints":  true
                                           },
                    "TransactionFetchRpc":  {
                                                "Enabled":  true,
                                                "QueueName":  "tx.fetch.rpc",
                                                "Prefetch": 25,
                                                "WriteAndForward":  true
                                            },
                    "TransactionFetchRpcSite":  {
                                                    "Enabled":  false,
                                                    "QueueName":  "tx.fetch.rpc.site",
                                                    "Prefetch": 1,
                                                    "WriteAndForward":  true
                                                },
                    "TransactionWrite":  {
                                             "Enabled":  true,
                                             "QueueName":  "tasks.tx.write.db",
                                             "Prefetch": 20
                                         },
                    "AssetFetch":  {
                                       "Enabled":  true,
                                       "QueueName":  "mint.fetch",
                                       "Prefetch": 5
                                   },
                    "AssetFetchDb":  {
                                         "Enabled":  true,
                                         "QueueName":  "mint.fetch.db",
                                         "Prefetch": 10
                                     },
                    "AssetFetchRpc":  {
                                          "Enabled":  true,
                                          "QueueName":  "mint.fetch.rpc",
                                          "Prefetch": 1,
                                          "WriteToDb":  true
                                      },
                    "OwnerFetchBatch":  {
                                            "Enabled":  true,
                                            "QueueName":  "owner.fetch.batch",
                                            "Prefetch": 1
                                        },
                    "PartyWrite":  {
                                       "Enabled":  true,
                                       "QueueName":  "party.write",
                                       "Prefetch": 50
                                   },
                    "MissingSymbol":  {
                                          "Enabled":  false,
                                          "IntervalSeconds":  60,
                                          "BatchSize":  10
                                      },
                    "UsageLog":  {
                                     "Enabled":  true,
                                     "QueueName":  "usage.log",
                                     "Prefetch": 50
                                 }
                },
    "Serilog":  {
                    "MinimumLevel":  {
                                         "Default":  "Information",
                                         "Override":  {
                                                          "T16O.Workers.TransactionFetchSiteWorkerService":  "Debug"
                                                      }
                                     },
                    "WriteTo":  [
                                    {
                                        "Name":  "Console"
                                    },
                                    {
                                        "Args":  {
                                                     "path":  "logs/t16o-site-queue-.log",
                                                     "retainedFileCountLimit":  7,
                                                     "rollingInterval":  "Day",
                                                     "outputTemplate":  "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] [{SourceContext}] {Message:lj}{NewLine}{Exception}"
                                                 },
                                        "Name":  "File"
                                    }
                                ]
                }
}
